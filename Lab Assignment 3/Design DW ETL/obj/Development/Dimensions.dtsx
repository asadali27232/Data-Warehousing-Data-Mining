      DTS:ObjectName="Date Dimension"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{47D2C4EA-16BA-4A24-B93D-65D314441690}"
          SQLTask:SqlStatementSource="-- Start and end dates for the date range to populate&#xA;DECLARE @startDate DATE = '2011-01-01';&#xA;DECLARE @endDate DATE = '2014-12-31';&#xA;&#xA;-- Variables for looping through the dates&#xA;DECLARE @currentDate DATE = @startDate;&#xA;DECLARE @year INT, @month INT, @day INT, @dayName VARCHAR(20), @dateId VARCHAR(8), @monthName VARCHAR(20), @fullDate VARCHAR(50);&#xA;&#xA;-- Loop through the dates and insert into the Date Dimension table&#xA;WHILE @currentDate &lt;= @endDate&#xA;BEGIN&#xA;    -- Extract year, month, day, and day name from the current date&#xA;    SET @year = YEAR(@currentDate);&#xA;    SET @month = MONTH(@currentDate);&#xA;    SET @day = DAY(@currentDate);&#xA;    SET @dayName = DATENAME(WEEKDAY, @currentDate);&#xA;    SET @monthName = DATENAME(MONTH, @currentDate);&#xA;&#xA;    -- Construct the date_id in the format &quot;YYYYMMDD&quot;&#xA;    SET @dateId = CONVERT(VARCHAR(8), @currentDate, 112);&#xA;&#xA;    -- Construct the full date string&#xA;    SET @fullDate = CONCAT(@dayName, ', ', @monthName, ' ', CAST(@day AS VARCHAR(2)), ', ', CAST(@year AS VARCHAR(4)));&#xA;&#xA;    -- Insert the date into the Date Dimension table&#xA;    INSERT INTO DateDimension (date_id, date, year, month, day, day_name, month_name, full_date)&#xA;    VALUES (@dateId, @currentDate, @year, @month, @day, @dayName, @monthName, @fullDate);&#xA;&#xA;    -- Increment the current date by one day&#xA;    SET @currentDate = DATEADD(DAY, 1, @currentDate);&#xA;END&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
